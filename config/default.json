{
    "App": {
        "port": 3000,
        "cors": {
            "origin": "*"
        },
        "database": {
            "mongoUrl": "mongodb://localhost:27017/surf-forecast"
        },
        "auth": {
            "key": "some-key",
            "tokenExpiresIn": "7 days"
        },
        "resources": {
            "StormGlass": {
                "apiUrl": "https://api.stormglass.io/v2",
                "apiToken": "do-not-hard-code",
                "cacheApiTtl": 3600
            }
        },
        "logger": {
            "enabled": true,
            "level": "info"
        }
    }
}