{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/src/index.js",
    "start:local": "yarn build && yarn start",
    "dev": "dotenv -e .env -- ts-node-dev  --ignore-watch node_modules \"./src/index.ts\" | npx pino-pretty -c -i \"hostname\"",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "test": "yarn lint && yarn test:unit && yarn test:functional",
    "test:unit": "dotenv -e .env -- jest",
    "test:watch": "dotenv -e .env -- jest --watch",
    "test:functional": "dotenv -e .env -- jest --projects ./test --runInBand",
    "ts:check-types": "tsc --noEmit"
  },
  "pre-commit": [
    "lint:fix",
    "ts:check-types",
    "test:unit",
    "test:functional"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
