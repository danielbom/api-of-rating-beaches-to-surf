{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsup --config tsconfig.build.json src/index.ts --clean --minify",
    "start": "dotenv -e .env -- node dist/index.js",
    "start:local": "yarn build && yarn start",
    "dev": "dotenv -e .env -- tsx watch \"./src/index.ts\"",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "test": "yarn lint && yarn test:unit && yarn test:functional",
    "test:unit": "dotenv -e .env -- jest",
    "test:watch": "dotenv -e .env -- jest --watch",
    "test:functional": "dotenv -e .env -- jest --projects ./test --runInBand",
    "ts:check": "tsc --noEmit"
  },
  "pre-commit": [
    "lint:fix",
    "ts:check",
    "test:unit",
    "test:functional"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@overnightjs/core": "^1.7.6",
    "axios": "^0.24.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.1",
    "compression": "^1.7.4",
    "config": "^3.3.9",
    "cors": "^2.8.5",
    "dotenv-cli": "^4.1.1",
    "express": "^4.18.2",
    "express-openapi-validator": "^5.0.1",
    "express-pino-logger": "^6.0.0",
    "express-rate-limit": "^5.5.0",
    "helmet": "^4.6.0",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "moment": "^2.29.4",
    "mongoose": "^6.8.4",
    "node-cache": "^5.1.2",
    "pino": "^7.11.0",
    "pino-pretty": "^9.1.1",
    "swagger-ui-express": "^4.6.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/config": "^0.0.40",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-pino-logger": "^4.0.2",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jest": "^27.0.2",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/lodash": "^4.14.176",
